<section class="contain">
   
    <hgroup>
        <h2 id="apropos">Art<span>icles</span>.</h2>
        <h3> Partage d'Expériences.</h3>
    </hgroup>

<section class="container-art">

    {{#each articles}} {{#compare categorie "Partage d éxpérience" operator="==="}}

    <figure class="blog"><a href="/art"></a> <img  class="artblog" src="{{this.image}}" alt="image enfance">
        <figcaption>
            <p class="titre-figcaption">{{this.titre}} </p>  <a href="/art/{{this._id}}">>Accédez à l'article</a>
        </figcaption>
    </figure>

    {{/compare}} {{/each}}
    
</section>
</section>